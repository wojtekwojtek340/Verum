<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Colors-->
    <Color x:Key="ColorPrimary1">#FF2C8ACB</Color>
    <Color x:Key="ColorPrimary2">#FF206FA6</Color>
    <Color x:Key="ColorPrimary3">#FF0B4166</Color>

    <!--Brushes-->
    <SolidColorBrush x:Key="BrushPrimary1" Color="{StaticResource ColorPrimary1}"/>
    <SolidColorBrush x:Key="BrushPrimary2" Color="{StaticResource ColorPrimary2}"/>
    <SolidColorBrush x:Key="BrushPrimary3" Color="{StaticResource ColorPrimary3}"/>
    <SolidColorBrush x:Key ="BrushPrimary2Trans" Color="LightGray" Opacity="0.13"/>
    
    
    
    
</ResourceDictionary>